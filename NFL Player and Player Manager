public class PlayerManager {

	   public static void main(String[] args) {}

		  public static class CreatePlayers {
			   public static void main(String args[]) {
				   int array [] = {1, 2, 3, 4, 5, 6};
			   
			for (int i = 0; i < array.length; i++) {
				System.out.print(array[i] + "Drew Brees");

			for (int i1 = 1; i1 < array.length; i1++) {
				System.out.print(array[i1] + "Matt Ryan");
					
			for (int i2 = 1; i2 < array.length; i2++) {
				System.out.print(array[i2] + "Kirk Cousins");
						
			for (int i3 = 1; i3 < array.length; i3++) {
				System.out.print(array[i3] + "Aaron Rodgers");
							
			for (int i4 = 1; i4 < array.length; i4++) {
				System.out.print(array[i4] + "Philip Rivers");
								
			for (int i5 = 1; i5 < array.length; i5++) {
				System.out.print(array[i5] + "Matthew Stafford");
									}
								}
							}
						}
					}
				}
